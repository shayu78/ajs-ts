{
  "name": "ajs-ts",
  "version": "1.0.0",
  "description": "Typescript",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production",
    "lint": "eslint .",
    "start": "webpack-dev-server --mode development",
    "test": "jest --coverage",
    "doc": "jsdoc -c jsdoc.conf.json",
